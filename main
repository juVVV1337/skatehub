local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "SKATEHUB",  IntroEnabled = true, IntroText = "Welcome to SKATEHUB!", IntroIcon = "rbxassetid://4483345998", Icon = "rbxassetid://4483345998", HidePremium = true, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Speed",
	Icon = "rbxassetid://10456489192",
	PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Speed Toggle"
})

OrionLib:MakeNotification({
	Name = "Successfully executed!",
	Content = "Thanks for using SKATEHUB, keep in mind that it's still in ALPHA.",
	Image = "rbxassetid://4483345998",
	Time = 5
})

Tab:AddButton({
	Name = "Legit Speed",
	Callback = function(s)
        local mt = getrawmetatable(game)
setreadonly(mt,false)
oldindex = mt.__index
mt.__index = newcclosure(function(t,k)
   if tostring(t) == "Humanoid" and tostring(k) == "WalkSpeed" then
       return 1
   end
   return oldindex(t,k)
end)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16.45
      		print("Executed Legit Speed.")
  	end    
})

local Tab = Window:MakeTab({
	Name = "Aimbot",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Aimbot Toggle"
})

Tab:AddButton({
	Name = "Aimbot",
	Callback = function(s)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/juVVV1337/camlockphenom/main/main"))()
      		print("Executed Aimbot.")
  	end    
})

local Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://10456489192",
	PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Credits:"
})

local Section = Tab:AddSection({
    Name = "UI: Orion Lib, thanks a lot guys."
})

local Section = Tab:AddSection({
    Name = "All of the built-in cheats were scripted by juv#0301."
})

local Section = Tab:AddSection({
    Name = "Our Discord Server: (depois meto)"
})
end)

OrionLib:Init()
